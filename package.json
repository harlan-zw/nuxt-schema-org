{
  "name": "nuxt-schema-org",
  "type": "module",
  "version": "5.0.8",
  "packageManager": "pnpm@10.16.1",
  "description": "The quickest and easiest way to build Schema.org graphs for Nuxt.",
  "author": "Harlan Wilton <harlan@harlanzw.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://nuxtseo.com/schema-org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-schema-org.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-schema-org/issues"
  },
  "keywords": [
    "schema-org",
    "nuxt",
    "@nuxt-schema-org",
    "nuxt-module",
    "nuxt3"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./schema": "./dist/schema.mjs",
    "./content": "./dist/content.mjs"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ],
      "content": [
        "./dist/content.d.mts"
      ],
      "schema": [
        "./dist/schema.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3030",
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && npm run client:build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "prepack": "pnpm run build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare client",
    "release": "pnpm build && bumpp -x \"npx changelogen --output=CHANGELOG.md\"",
    "typecheck": "tsc --noEmit ",
    "test": "nuxi prepare test/fixtures/i18n && nuxi prepare test/fixtures/i18n-domains && vitest",
    "test:attw": "attw --pack"
  },
  "peerDependencies": {
    "@unhead/vue": "^2.0.7",
    "unhead": "^2.0.7"
  },
  "peerDependenciesMeta": {
    "@unhead/vue": {
      "optional": true
    },
    "unhead": {
      "optional": true
    }
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.2",
    "@unhead/schema-org": "^2.0.14",
    "defu": "^6.1.4",
    "nuxt-site-config": "^3.2.5",
    "pathe": "^2.0.3",
    "pkg-types": "^2.3.0",
    "sirv": "^3.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@arethetypeswrong/cli": "^0.18.2",
    "@nuxt/content": "^3.7.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.2",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^3.3.4",
    "@nuxtjs/i18n": "^10.1.0",
    "@nuxtjs/robots": "^5.5.3",
    "better-sqlite3": "^12.2.0",
    "bumpp": "^10.2.3",
    "cheerio": "^1.1.2",
    "eslint": "^9.35.0",
    "nuxt": "^4.1.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "build": {
    "externals": [
      "@unhead/vue/types"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "vue-demi"
    ]
  }
}
